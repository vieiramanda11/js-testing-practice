{
  "parserOptions": { 
    "ecmaVersion": 8,
  },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
    'import/extensions': 'off',
    "import/no-unresolved": "off",
    "import/no-named-as-default": 0
    
  },
  "env": {
   "browser": true,
   "es6": true,
   "jest": true
  },  
}